{$R *.dfm}

procedure TForm35.Button2Click(Sender: TObject);
begin
form34.adotable1.Insert;
form34.ADOTable1Type_Siege.value:=combobox1.text;
form34.ADOTable1Mail_Siege.value:=edit3.text;
form34.ADOTable1Lieu_Siege.value:=edit4.text;
form34.adotable1.Post;
edit1.text:='';
edit2.text:='';
edit3.text:='';
edit4.text:='';
close;
end;

procedure TForm35.Button3Click(Sender: TObject);
begin
Close;
end;

end.
